{
    "python.defaultInterpreterPath": "venv/bin/python",
    "files.exclude": {
      ".vscode": true,
      "**/venv": true,
      "**/.venv": true,
      "**/__pycache__": true,
    },
    "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.formatOnSave": true
    },
    "python.formatting.provider": "none",
    "emmet.includeLanguages": {"jinja-html": "html"},
    "files.associations": {
      "*.html": "jinja-html"
     },
  }